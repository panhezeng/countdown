{
  "name": "@panhezeng/countdown",
  "description": "countdown",
  "version": "2.0.1",
  "author": "潘何增 <panhezeng@gmail.com>",
  "license": "MIT",
  "private": false,
  "keywords": [
    "countdown",
    "javascript"
  ],
  "homepage": "https://github.com/panhezeng/countdown-js#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/panhezeng/countdown-js.git"
  },
  "files": [
    "dist",
    "src",
    "package.json",
    "README"
  ],
  "main": "dist/countdown.min.js",
  "scripts": {
    "dev:example": "webpack-dev-server --mode development --open --hot --config webpack.example.config.js",
    "build:example": "webpack --mode production --config webpack.example.config.js",
    "build": "webpack --mode production"
  },
  "devDependencies": {
    "react": "~16",
    "react-dom": "~16",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.3",
    "prettier": "^1.16.4",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@panhezeng/before-run-webpack-plugin": "^1.0.7",
    "babel-loader": "^8.0.5",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md,jsx,vue,ts,less}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "**/dist/**",
      "**/docs/**"
    ]
  },
  "engines": {
    "node": ">= 10.0.0",
    "npm": ">= 6.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
